{
  "name": "nexustrade-supervisor",
  "version": "1.0.0",
  "description": "NexusTrade Platform - Supervisor (Production Ready)",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:ws": "next dev & node -e \"setTimeout(() => fetch('http://localhost:3000/api/ws'), 3000)\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:ws": "next start & node -e \"setTimeout(() => fetch('http://localhost:3000/api/ws'), 3000)\"",
    "lint": "next lint --fix",
    "lint:strict": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run type-check && pnpm run lint:strict && pnpm run build",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "seed": "node prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "precommit": "pnpm run lint && pnpm run type-check"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "5.7.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@types/ws": "^8.18.1",
    "@upstash/redis": "^1.35.6",
    "@vercel/blob": "^2.0.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.0",
    "node-cron": "^4.2.1",
    "openai": "^4.32.0",
    "openai-fetch": "^3.4.2",
    "postcss": "^8.5.6",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.10.3",
    "redis": "^5.8.3",
    "speakeasy": "^2.0.0",
    "stripe": "^14.12.0",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^4.1.14",
    "tailwindcss-animate": "^1.0.7",
    "twelvedata": "^3.0.1",
    "ws": "^8.18.3",
    "yahoo-finance2": "^3.10.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "prisma": "5.7.0",
    "typescript": "^5.3.3"
  }
}
